{
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "6.0",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@infra/*": [
        "src/infra/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ],
      "@model/*": [
        "src/domain/model/*"
      ],
      "@usecase/*": [
        "src/domain/usecase/*"
      ],
      "@presentation/*": [
        "src/presentation/*"
      ],
      "@i18n": [
        "i18n/index"
      ],
      "@i18n/*": [
        "i18n/*"
      ],
      "@theme/*": [
        "src/presentation/shared/theme/*"
      ],
      "@hooks/*": [
        "src/presentation/hooks/*"
      ],
      "@shared/*": [
        "src/presentation/shared/*"
      ],
      "@screens/*": [
        "src/presentation/screens/*"
      ],
      "@constants/*": [
        "src/presentation/shared/constants/*"
      ],
      "@components/*": [
        "src/presentation/shared/components/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "*.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
